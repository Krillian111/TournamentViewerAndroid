<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="de.tum.kickercoding.tournamentviewer">

    <application
        android:name=".TournamentViewerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".setup.monsterdyp.MonsterDypBasicSetupActivity"/>
        <activity android:name=".setup.monsterdyp.MonsterDypPlayerSetupActivity"/>
        <activity android:name=".tournament.monsterdyp.MonsterDypTournamentActivity"/>
        <activity android:name=".setup.monsterdyp.TestActivity">
        </activity>
    </application>

</manifest>